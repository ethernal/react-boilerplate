{
  "name"       : "react-boilerplate",
  "version"    : "0.15.0",
  "description": "React Boilerplate with Typescript, Babel, Jest, EsLint, Prettier, Styled Components, React Testing Library, Webpack, Webpack Dev Server",
  "main"       : "index.tsx",
  "scripts"    : {
    "typings"       : "tcm -p \"src/**/*.+(css|scss)\" --camelCase",
    "typings:watch" : "tcm -p \"src/**/*.+(css|scss)\" --camelCase --watch",
    "dev"           : "webpack --mode development --watch",
    "format"        : "prettier \"**/*.(js|ts|tsx|jsx)\" --write",
    "check:lint"    : "eslint . --ext \".js,.ts,.tsx,.jsx\"",
    "check:style"   : "stylelint-config-prettier-check",
    "check:type"    : "tsc",
    "docz:dev"      : "docz dev",
    "docz:build"    : "docz build",
    "test"          : "is-ci \"test:coverage\" \"test:watch\"",
    "test:once"     : "jest --no-cache",
    "test:watch"    : "jest --runInBand --watch",
    "test:debug"    : "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:coverage" : "jest --coverage",
    "prestatic:test": "yarn build",
    "prestatic:prod": "yarn build:prod",
    "test:e2e"      : "is-ci \"test:e2e:prod\" \"test:e2e:dev\"",
    "test:e2e:prod" : "start-server-and-test static:prod http://localhost:8080 cy:run",
    "test:e2e:dev"  : "start-server-and-test static:dev http://localhost:8080 cy:open",
    "cy:open"       : "cypress open",
    "cy:run"        : "cypress run",
    "build"         : "cross-env NODE_ENV=development webpack --config webpack.config.js && npm run typings",
    "build:prod"    : "cross-env NODE_ENV=production webpack --config webpack.config.js && npm run typings",
    "server:watch"  : "nodemon --watch webpack.config.js --watch src --watch cypress -x cross-env NODE_ENV=development webpack-dev-server",
    "server:dev"    : "cross-env NODE_ENV=development webpack-dev-server",
    "server:prod"   : "cross-env NODE_ENV=production webpack-dev-server --config webpack.config.js",
    "static:test"   : "serve --no-clipboard --single --listen 8080 dist",
    "static:dev"    : "nodemon -e js,ts,tsx,jsx,scss,css,html --watch src -x npm run static:test",
    "static:prod"   : "serve --no-clipboard --single --listen 8080 dist",
    "validate"      : "npm run build:prod && npm run build && npx jest --config ./test/jest.tsc.js && npx jest --config ./test/jest.lint.js && npx jest --config ./test/jest.style.js && npm run test:coverage && npx jest --config ./test/jest.test.js && npm run test:e2e:run && npm run docz:build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author"      : "Sebastian Pieczy≈Ñski",
  "license"     : "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@emotion/core"  : "^10.0.5",
    "@reach/router"  : "^1.2.1",
    "emotion-theming": "^10.0.6",
    "react"          : "^16.8.0-alpha.1 ",
    "react-dom"      : "^16.7.0",
    "styled-system"  : "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli"                                  : "^7.2.3",
    "@babel/core"                                 : "^7.2.2",
    "@babel/parser"                               : "^7.2.3",
    "@babel/plugin-proposal-class-properties"     : "^7.2.3",
    "@babel/plugin-syntax-dynamic-import"         : "^7.2.0",
    "@babel/preset-env"                           : "^7.2.3",
    "@babel/preset-react"                         : "^7.0.0",
    "@babel/preset-typescript"                    : "^7.1.0",
    "@babel/register"                             : "^7.0.0",
    "@mapbox/rehype-prism"                        : "^0.3.1",
    "@types/jest"                                 : "^23.3.10",
    "@types/jest-in-case"                         : "^1.0.0",
    "@types/node"                                 : "^10.12.18",
    "@types/react"                                : "^16.7.17",
    "@types/react-dom"                            : "^16.0.11",
    "@types/webpack"                              : "^4.4.22",
    "babel-loader"                                : "^8.0.4",
    "babel-plugin-dynamic-import-node"            : "^2.2.0",
    "babel-plugin-emotion"                        : "^10.0.5",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "case-sensitive-paths-webpack-plugin"         : "^2.2.0",
    "chalk"                                       : "^2.4.2",
    "clean-webpack-plugin"                        : "^1.0.0",
    "copy-webpack-plugin"                         : "^4.6.0",
    "create-jest-runner"                          : "^0.4.1",
    "cross-env"                                   : "^5.2.0",
    "css-loader"                                  : "^2.1.0",
    "cypress"                                     : "^3.1.4",
    "cypress-testing-library"                     : "^2.3.5",
    "docz"                                        : "latest",
    "docz-plugin-css"                             : "^0.11.0",
    "docz-theme-default"                          : "^0.13.7",
    "eslint"                                      : "^5.10.0",
    "eslint-import-resolver-jest"                 : "^2.1.1",
    "eslint-plugin-cypress"                       : "^2.2.0",
    "eslint-plugin-import"                        : "^2.14.0",
    "eslint-plugin-typescript"                    : "^0.14.0",
    "file-loader"                                 : "^3.0.1",
    "html-loader"                                 : "^0.5.5",
    "html-minifier"                               : "^3.5.21",
    "html-webpack-plugin"                         : "^3.2.0",
    "husky"                                       : "^1.3.1",
    "identity-obj-proxy"                          : "^3.0.0",
    "image-webpack-loader"                        : "^4.6.0",
    "is-ci-cli"                                   : "^1.1.1",
    "jest"                                        : "^24.0.0",
    "jest-emotion"                                : "^10.0.6",
    "jest-in-case"                                : "^1.0.2",
    "jest-resolve"                                : "^24.0.0",
    "jest-runner-eslint"                          : "^0.7.1",
    "jest-runner-stylelint"                       : "^1.0.0",
    "jest-runner-tsc"                             : "^1.3.2",
    "jest-watch-select-projects"                  : "^0.1.1",
    "jest-watch-typeahead"                        : "^0.2.0",
    "lint-staged"                                 : "^8.1.0",
    "mini-css-extract-plugin"                     : "^0.5.0",
    "node-sass"                                   : "^4.11.0",
    "nodemon"                                     : "^1.18.9",
    "npm-run-all"                                 : "^4.1.5",
    "pluggable-babel-eslint"                      : "^0.3.0",
    "postcss-loader"                              : "^3.0.0",
    "postcss-preset-env"                          : "^6.5.0",
    "prettier"                                    : "^1.15.3",
    "prettier-stylelint"                          : "^0.4.2",
    "react-hot-loader"                            : "^4.6.3",
    "react-testing-library"                       : "^5.4.2",
    "sass-loader"                                 : "^7.1.0",
    "serve"                                       : "^10.1.1",
    "source-map-loader"                           : "^0.2.4",
    "start-server-and-test"                       : "^1.7.11",
    "style-loader"                                : "^0.23.1",
    "stylelint"                                   : "^9.9.0",
    "stylelint-config-prettier"                   : "^4.0.0",
    "stylelint-config-standard"                   : "^18.2.0",
    "stylelint-webpack-plugin"                    : "^0.10.5",
    "typed-css-modules"                           : "^0.3.7",
    "typescript"                                  : "^3.2.2",
    "typescript-babel-jest"                       : "^1.0.5",
    "typescript-eslint-parser"                    : "^21.0.2",
    "typings-for-css-modules-loader"              : "^1.7.0",
    "url-loader"                                  : "^1.1.2",
    "watch"                                       : "^1.0.2",
    "webpack"                                     : "^4.28.3",
    "webpack-cli"                                 : "^3.1.2",
    "webpack-dev-server"                          : "^3.1.14"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  }
}
